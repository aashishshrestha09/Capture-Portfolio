sers\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\components\\Nav.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\pages\\ContactUs.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\pages\\OurWork.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\pages\\MovieDetail.js",["79"],"import React, {useState, useEffect} from 'react'\r\nimport styled from 'styled-components'\r\nimport {useHistory} from 'react-router-dom'\r\nimport {MovieState} from '../movieState'\r\n// Animations\r\nimport {motion} from 'framer-motion'\r\nimport {pageAnimation} from '../animation'\r\n\r\nconst MovieDetail = () => {\r\n    const history = useHistory();\r\n    const url = history.location.pathname;\r\n    const [movies, setMovies] = useState(MovieState);\r\n    const [movie, setMovie] = useState(null)\r\n\r\n    // UseEffect\r\n    useEffect(() => {\r\n        const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\r\n        setMovie(currentMovie[0]);\r\n    }, [movies, url]);\r\n\r\n    return (\r\n        <>\r\n            {movie && (\r\n            <Details exit=\"exit\" variants={pageAnimation} initial=\"hidden\" animate=\"show\">\r\n                <HeadLine>\r\n                    <h2>{movie.title}</h2>   \r\n                    <img src={movie.mainImg} alt=\"movie\" /> \r\n                </HeadLine> \r\n                <Awards>\r\n                    {movie.awards.map(award => (\r\n                        <Award key={award.title} title={award.title} description={award.description} />\r\n                    ))}\r\n                </Awards>\r\n                <ImageDisplay>\r\n                    <img src={movie.secondaryImg} alt=\"movie\" />\r\n                </ImageDisplay>           \r\n            </Details>\r\n            )}\r\n        </>\r\n    )\r\n}\r\n\r\nconst Details = styled(motion.div)`\r\n    color: white;\r\n`\r\nconst HeadLine = styled.div`\r\n    min-height: 90vh;\r\n    padding-top: 20vh;\r\n    position: relative;\r\n    h2 {\r\n        position: absolute;\r\n        top: 10%;\r\n        left: 50%;\r\n        transform: translate(-50%, -10%);\r\n    }\r\n    img {\r\n        width: 100%;\r\n        height: 70vh;\r\n        object-fit: cover;\r\n    }\r\n`\r\nconst Awards = styled.div`\r\n    min-height: 80vh;\r\n    display: flex;\r\n    margin: 5rem 3rem;\r\n    align-items: center;\r\n    justify-content: space-around;\r\n    @media (max-width: 1060px) {\r\n        display: block;\r\n        margin: 0.5rem 0.5rem;\r\n    }\r\n`\r\n\r\nconst AwardStyle = styled.div`\r\n    padding: 5rem;\r\n    h3 {\r\n        font-size: 2rem;\r\n    }\r\n    .line {\r\n        width: 100%;\r\n        background: #23d997;\r\n        height: 0.5rem;\r\n        margin: 1rem 0rem;\r\n    }\r\n    p {\r\n        padding: 2rem 0rem;\r\n    }\r\n`\r\nconst ImageDisplay = styled.div`\r\n    min-height: 50vh;\r\n    img {\r\n        width: 100%;\r\n        height: 100vh;\r\n        object-fit: cover;\r\n    }\r\n`\r\n\r\n// Award Component\r\nconst Award = ({title, description}) => {\r\n    return (\r\n        <AwardStyle>\r\n            <h3>{title}</h3>\r\n            <div className=\"line\"></div>\r\n            <p>{description}</p>\r\n        </AwardStyle>\r\n    )\r\n}\r\n\r\nexport default MovieDetail;","C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\movieState.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\animation.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\components\\Wave.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\components\\Toogle.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\components\\useScroll.js",[],"C:\\Users\\DELL\\Documents\\Creative React\\CHAPTER 4\\capture\\src\\components\\ScrollTop.js",[],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId